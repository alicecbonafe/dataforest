{
    "name": "alicebonafe/dataforest",
    "description": "A Laravel Livewire data table component with advanced features",
    "version": "0.0.1",
    "keywords": [
        "laravel",
        "livewire",
        "datatable",
        "table",
        "data-grid"
    ],
    "homepage": "https://github.com/alicebonafe/dataforest",
    "license": "MIT",
    "authors": [
        {
            "name": "Alice Bonafe",
            "email": "alicecbonafe@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "livewire/livewire": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dataforest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dataforest\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dataforest\\Providers\\DataforestServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}